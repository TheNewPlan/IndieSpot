<p id="notice"><%= notice %></p>

<h1>Music</h1>

<table>
  <thead>
    <tr>
      <th>Song</th>
      <th>Album</th>
      <th>Band</th>
      <th>Genre</th>
      <th>Release</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= music.song %></td>
        <td><%= music.album %></td>
        <td><%= music.band %></td>
        <td><%= music.genre %></td>
        <td><%= music.release %></td>
        <td><%= link_to 'Show', music %></td>
        <td><%= link_to 'Edit', edit_music_path(music) %></td>
        <td><%= link_to 'Destroy', music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Music', new_music_path %>
