<p id="notice"><%= notice %></p>

<h1>Music</h1>

<table>
  <thead>
    <tr>
      <th><%= link_to "Song", music_view_songSort_path %></th>
      <th><%= link_to "Album", music_view_albumSort_path %></th>
      <th><%= link_to "Band", music_view_bandSort_path %></th>
      <th><%= link_to "Genre", music_view_genreSort_path %></th>
      <th><%= link_to "Release", music_view_releaseSort_path %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= music.song %></td>
        <td><%= music.album %></td>
        <td><%= music.band %></td>
        <td><%= music.genre %></td>
        <td><%= music.release %></td>
        <td><%= link_to 'Show', music %></td>
        <td><%= link_to 'Edit', edit_music_path(music) %></td>
        <td><%= link_to 'Destroy', music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Music', new_music_path %>
